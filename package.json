{
  "name": "gmu-advance-toolkit",
  "version": "1.0.10",
  "scripts": {
    "ng": "ng",
    "prestart": "node version.ts",
    "start": "npm run generate && ng serve",
    "prebuild:dev": "node version.ts",
    "build:dev": "npm run clean && npm run generate && ng build --configuration=dev",
    "build:staging": "npm run clean && npm run generate && ng build --configuration=staging",
    "build:prod": "npm run clean && npm run generate && ng build --configuration=production",
    "clean": "rimraf dist",
    "build:stats": "npm run build:stats:prod   && npm run analyze",
    "build:stats:prod": "npm run clean:stats   && npm run generate && ng build --configuration=production --stats-json --output-path dist/stats",
    "precommit": "git add .",
    "commit": "npx git-cz",
    "clean:stats": "rimraf dist/stats",
    "analyze": "webpack-bundle-analyzer ./dist/stats/stats-es2015.json",
    "generate": "graphql-codegen --config codegen.yml",
    "test": "ng test",
    "test-headless": "npm run generate && ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.9",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.0.9",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/router": "~10.0.9",
    "@larscom/ngrx-store-storagesync": "^6.1.7",
    "@ngrx/data": "^9.2.0",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/entity": "^9.2.0",
    "@ngrx/router-store": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@vantage-point/maps": "^1.0.9",
    "apollo-angular": "^1.10.0",
    "apollo-angular-link-http": "^1.11.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "current-device": "^0.10.1",
    "date-fns": "^2.14.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.4",
    "highcharts": "^8.1.2",
    "lodash": "^4.17.19",
    "ngx-currency": "^2.3.3",
    "ngx-custom-validators": "^8.0.0",
    "ngx-mask": "^9.1.2",
    "ngx-spinner": "^10.0.1",
    "proj4": "^2.6.2",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "uuid": "^8.2.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.6",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.9",
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/introspection": "^1.17.0",
    "@graphql-codegen/typescript": "^1.17.0",
    "@graphql-codegen/typescript-apollo-angular": "^1.17.0",
    "@graphql-codegen/typescript-operations": "^1.17.0",
    "@ngrx/schematics": "^9.2.0",
    "@types/date-fns": "^2.6.0",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "foundation-sites": "^6.6.3",
    "fs-extra": "^9.0.1",
    "git-describe": "^4.0.4",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "path": "^0.12.7",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
