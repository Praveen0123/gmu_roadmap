{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm",
    "defaultCollection": "@ngrx/schematics",
    "analytics": false
  },
  "newProjectRoot": "projects",
  "projects": {
    "gmu-advance-toolkit": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@ngrx/schematics:class": {
          "skipTests": true
        },
        "@ngrx/schematics:directive": {
          "skipTests": true
        },
        "@ngrx/schematics:guard": {
          "skipTests": true
        },
        "@ngrx/schematics:module": {
          "skipTests": true
        },
        "@ngrx/schematics:service": {
          "skipTests": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "gmu",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "lodash",
              "graphql-tag",
              "highcharts",
              "highcharts/highmap",
              "highcharts/modules/map",
              "zen-observable"
            ],
            "outputPath": "dist/gmu-advance-toolkit",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/robots.txt"
            ],
            "styles": [
              "src/scss/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/scss",
                "src/scss/common"
              ]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/app/root-store/_build-specifics/index.ts",
                  "with": "src/app/root-store/_build-specifics/index.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                },
                {
                  "replace": "src/app/root-store/_build-specifics/index.ts",
                  "with": "src/app/root-store/_build-specifics/index.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "gmu-advance-toolkit:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "gmu-advance-toolkit:build:production"
            },
            "dev": {
              "browserTarget": "gmu-advance-toolkit:build:dev"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "gmu-advance-toolkit:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/robots.txt"
            ],
            "styles": [
              "src/scss/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/scss",
                "src/scss/common"
              ]
            },
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "gmu-advance-toolkit:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gmu-advance-toolkit:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "gmu-advance-toolkit",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "gmu",
      "style": "scss"
    },
    "@ngrx/schematics:component": {
      "prefix": "gmu",
      "styleext": "scss"
    },
    "@ngrx/schematics:directive": {
      "prefix": "gmu"
    }
  }
}